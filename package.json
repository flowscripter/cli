        "rollup-plugin-node-resolve": "5.2.0",
        "rollup-plugin-peer-deps-external": "2.2.3",
        "rollup-plugin-typescript2": "0.27.2",
        "semantic-release": "17.1.1",
        "sonarqube-scanner": "2.7.0",
        "temp-dir": "2.0.0",
        "ts-jest": "26.2.0",
        "typescript": "3.9.7"
    },
    "dependencies": {
        "@flowscripter/cli-framework": "5.3.0",
